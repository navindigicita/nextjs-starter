{
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["*.{png,jpg,gif,svg}", "*.css"]
    },

    "routes": [
      {
        "route": "/pages/",
       "headers": {
        "content-type": "application/javascript"
       }
      },
      {
        "route": "/chunks/",
       "headers": {
        "content-type": "application/javascript"
       }
      },
      {
        "route": "/css/",
        "headers": {
          "content-type": "text/css"
         }
      }
    ],
    "mimeTypes": {
      ".json": "text/json",
      ".css":"text/css",
      ".js": "application/javascript"
    }

}