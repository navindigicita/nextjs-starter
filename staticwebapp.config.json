{
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["*.{png,jpg,gif,svg}", "*.css","*.js","*.jsx"]
    },

    "routes": [
      {
        "route": "/pages/",
       "headers": {
        "content-type": "application/javascript"
       }
      },
      {
        "route": "/chunks/",
       "headers": {
        "content-type": "application/javascript"
       }
      },
      {
        "route": "/css/",
        "headers": {
          "content-type": "text/css"
         }
      }
    ]

}