{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "*.{png,jpg,gif,svg}",
      "*.css"
    ]
  },
  "routes": [
    {
      "route": "/pages/*",
      "headers": {
        "content-type": "application/javascript"
      }
    },
    {
      "route": "/login",
      "headers": {
        "content-type": "application/javascript"
      }
    },
    {
      "route": "/chunks/",
      "headers": {
        "content-type": "application/javascript"
      }
    },
    {
      "route": "/css/",
      "headers": {
        "content-type": "text/css"
      }
    }
  ],
  
  "globalHeaders": {
    "content-type": "application/json"
  }
}